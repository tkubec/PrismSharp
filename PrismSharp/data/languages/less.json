    "greedy": true,
    "inside": {
      "function": "r:/^url/i",
      "punctuation": "r:/^\\(|\\)(?:(?=\\r$)|$)/",
      "string": {
        "pattern": "r:/^(?:\"(?:\\\\(?:\\r\\n|[\\s\\S])|[^\"\\\\\\r\\n])*\"|'(?:\\\\(?:\\r\\n|[\\s\\S])|[^'\\\\\\r\\n])*')(?:(?=\\r$)|$)/",
        "alias": "s:url"
