{
  "comment": {
    "pattern": "r:/#[^\\r\\n]*/",
    "greedy": true
  },
  "table": {
    "pattern": "r:/(^[\\t ]*\\[\\s*(?:\\[\\s*)?)(?:[\\w-]+|'[^'\\n\\r]*'|\"(?:\\\\[^\\r\\n]|[^\\\\\"\\r\\n])*\")(?:\\s*\\.\\s*(?:[\\w-]+|'[^'\\n\\r]*'|\"(?:\\\\[^\\r\\n]|[^\\\\\"\\r\\n])*\"))*(?=\\s*\\])/m",
    "lookbehind": true,
    "greedy": true,
    "alias": "s:class-name"
  },
  "key": {
    "pattern": "r:/(^[\\t ]*|[{,]\\s*)(?:[\\w-]+|'[^'\\n\\r]*'|\"(?:\\\\[^\\r\\n]|[^\\\\\"\\r\\n])*\")(?:\\s*\\.\\s*(?:[\\w-]+|'[^'\\n\\r]*'|\"(?:\\\\[^\\r\\n]|[^\\\\\"\\r\\n])*\"))*(?=\\s*=)/m",
    "lookbehind": true,
    "greedy": true,
    "alias": "s:property"
  },
  "string": {
    "pattern": "r:/\"\"\"(?:\\\\[\\s\\S]|[^\\\\])*?\"\"\"|'''[\\s\\S]*?'''|'[^'\\n\\r]*'|\"(?:\\\\[^\\r\\n]|[^\\\\\"\\r\\n])*\"/",
    "greedy": true
  },
  "date": [
    {
      "pattern": "r:/\\b\\d{4}-\\d{2}-\\d{2}(?:[T\\s]\\d{2}:\\d{2}:\\d{2}(?:\\.\\d+)?(?:Z|[+-]\\d{2}:\\d{2})?)?\\b/i",
      "alias": "s:number"
    },
    {
      "pattern": "r:/\\b\\d{2}:\\d{2}:\\d{2}(?:\\.\\d+)?\\b/",
      "alias": "s:number"
    }
  ],
  "number": "r:/(?:\\b0(?:x[\\da-zA-Z]+(?:_[\\da-zA-Z]+)*|o[0-7]+(?:_[0-7]+)*|b[10]+(?:_[10]+)*))\\b|[-+]?\\b\\d+(?:_\\d+)*(?:\\.\\d+(?:_\\d+)*)?(?:[eE][+-]?\\d+(?:_\\d+)*)?\\b|[-+]?\\b(?:inf|nan)\\b/",
  "boolean": "r:/\\b(?:false|true)\\b/",
  "punctuation": "r:/[.,=[\\]{}]/"
}